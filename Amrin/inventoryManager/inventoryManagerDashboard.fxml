<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.oop.groupthirteen.group13simulatingoperationsofwholesaleclub.Amrin.inventoryManager.inventoryManagerDashboardController">
    <children>
        <Text layoutX="167.0" layoutY="54.0" text="Inventory Manager">
            <font>
                <Font name="System Bold" size="32.0" />
            </font>
        </Text>

        <Button fx:id="logoutButton" layoutX="41.0" layoutY="29.0"
                mnemonicParsing="false"
                text="Logout"
                onAction="#handleLogout"/>

        <Button fx:id="incomingShipmentButton" layoutX="41.0" layoutY="81.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Incoming Shipment"
                onAction="#handleIncomingShipment"/>

        <Button fx:id="trackDamagedButton" layoutX="41.0" layoutY="162.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Track Damaged"
                onAction="#handleTrackDamaged"/>

        <Button fx:id="stockAuditButton" layoutX="41.0" layoutY="235.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Perform Stock Audit"
                onAction="#handleStockAudit"/>

        <Button fx:id="assignWarehouseButton" layoutX="41.0" layoutY="306.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Assign Warehouse Space"
                onAction="#handleAssignWarehouse"/>

        <Button fx:id="checkStockButton" layoutX="360.0" layoutY="81.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Check Current Stock"
                onAction="#handleCheckStock"/>

        <Button fx:id="updateExpiryButton" layoutX="360.0" layoutY="162.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Update Product Expiry Data"
                onAction="#handleUpdateExpiry"/>

        <Button fx:id="approveReturnButton" layoutX="360.0" layoutY="235.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="Approve Return-to-Vendor"
                onAction="#handleApproveReturn"/>

        <Button fx:id="viewReportButton" layoutX="360.0" layoutY="306.0"
                mnemonicParsing="false"
                prefHeight="39.0" prefWidth="221.0"
                text="View Supplier Performance Report"
                onAction="#handleViewReport"/>
    </children>
</AnchorPane>
