<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.oop.groupthirteen.group13simulatingoperationsofwholesaleclub.Bashar.Cashier.manageCashController">

    <children>
        <TextField fx:id="cashInField" layoutX="65.0" layoutY="41.0" prefHeight="26.0" prefWidth="246.0" promptText="cash-in"/>
        <TextField fx:id="cashOutField" layoutX="392.0" layoutY="42.0" prefHeight="26.0" prefWidth="246.0" promptText="cash-Out"/>
        <DatePicker fx:id="validateDatePicker" layoutX="66.0" layoutY="92.0" prefHeight="26.0" prefWidth="244.0" promptText="Validate"/>

        <Button fx:id="saveButton" layoutX="388.0" layoutY="93.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="257.0" text="Save" onAction="#handleSave"/>
        <Button fx:id="backButton" layoutX="260.0" layoutY="460.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="298.0" text="Back" onAction="#handleBack"/>

        <Label fx:id="confirmationLabel" layoutX="67.0" layoutY="140.0" prefHeight="60.0" prefWidth="632.0" text="In there cash in"/>

        <TableView fx:id="cashTable" layoutX="19.0" layoutY="221.0" prefHeight="200.0" prefWidth="761.0">
            <columns>
                <TableColumn fx:id="cashInColumn" prefWidth="245.0" text="cash-in"/>
                <TableColumn fx:id="cashOutColumn" prefWidth="269.0" text="cash-Out"/>
                <TableColumn fx:id="validateColumn" prefWidth="245.0" text="Validate"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
