<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.oop.groupthirteen.group13simulatingoperationsofwholesaleclub.Bashar.accountOfficer.approveTrackSupplierController">

    <children>
        <Button fx:id="backButton" layoutX="225.0" layoutY="457.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="26.0" prefWidth="331.0" text="Back" />

        <TextField fx:id="supplierNameField" layoutX="40.0" layoutY="37.0" prefHeight="26.0" prefWidth="283.0" promptText="Name of Supplier" />

        <ComboBox fx:id="paymentTypeCombo" layoutX="384.0" layoutY="38.0" prefHeight="26.0" prefWidth="322.0" promptText="Payment Type" />

        <TextField fx:id="totalAmountField" layoutX="39.0" layoutY="85.0" prefHeight="26.0" prefWidth="283.0" promptText="Total Amount" />

        <Button fx:id="payButton" layoutX="380.0" layoutY="87.0" mnemonicParsing="false" onAction="#handlePay" prefHeight="26.0" prefWidth="331.0" text="Pay" />

        <TreeTableView fx:id="supplierTable" layoutX="30.0" layoutY="150.0" prefHeight="294.0" prefWidth="750.0">
            <columns>
                <TreeTableColumn fx:id="nameColumn" prefWidth="211.0" text="Name of Supplier" />
                <TreeTableColumn fx:id="paymentColumn" prefWidth="246.0" text="Payment Type" />
                <TreeTableColumn fx:id="amountColumn" prefWidth="291.0" text="Total Amount" />
            </columns>
        </TreeTableView>
    </children>
</AnchorPane>
