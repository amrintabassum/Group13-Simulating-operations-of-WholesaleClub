<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.oop.groupthirteen.group13simulatingoperationsofwholesaleclub.Bashar.accountOfficer.accountOfficerController">

    <children>
        <Text layoutX="254.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Account Officer">
            <font><Font size="42.0" /></font>
        </Text>

        <Button fx:id="logoutButton" layoutX="202.0" layoutY="414.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="430.0" text="Logout" onAction="#handleLogout"/>

        <Button fx:id="recordPurchaseButton" layoutX="79.0" layoutY="100.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Record Inventory Purchase" onAction="#handleRecordInventoryPurchase"/>

        <Button fx:id="generateRevenueButton" layoutX="443.0" layoutY="98.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Generate Revenue Reports" onAction="#handleGenerateRevenueReports"/>

        <Button fx:id="approveSupplierButton" layoutX="78.0" layoutY="181.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Approve and Track Supplier" onAction="#handleApproveTrackSupplier"/>

        <Button fx:id="maintainPayrollButton" layoutX="442.0" layoutY="179.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Maintain Employee Payroll" onAction="#handleMaintainEmployeePayroll"/>

        <Button fx:id="trackPaymentDuesButton" layoutX="77.0" layoutY="273.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text=" Track Payment Dues " onAction="#handleTrackPaymentDues"/>

        <Button fx:id="reviewTransactionsButton" layoutX="441.0" layoutY="271.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Review Past Transactions" onAction="#handleReviewPastTransactions"/>

        <Button fx:id="uploadDocumentsButton" layoutX="76.0" layoutY="354.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Upload Financial Documents" onAction="#handleUploadFinancialDocuments"/>

        <Button fx:id="generateExpensesButton" layoutX="440.0" layoutY="352.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="311.0" text="Generate Expenses Reports" onAction="#handleGenerateExpensesReports"/>
    </children>
</AnchorPane>
