<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.oop.groupthirteen.group13simulatingoperationsofwholesaleclub.Bashar.Cashier.CashierdashboardController">

    <children>
        <Button fx:id="registerCustomerButton" layoutX="89.0" layoutY="91.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Register customer" onAction="#handleRegisterCustomer"/>

        <Button fx:id="applyDiscountsButton" layoutX="400.0" layoutY="91.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Apply discounts" onAction="#handleApplyDiscounts"/>

        <Button fx:id="salesSummaryButton" layoutX="87.0" layoutY="153.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Sales Summary" onAction="#handleSalesSummary"/>

        <Button fx:id="manageCashButton" layoutX="398.0" layoutY="153.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Manage Cash" onAction="#handleManageCash"/>

        <Button fx:id="processReturnsButton" layoutX="86.0" layoutY="225.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Process Customer Returns" onAction="#handleProcessReturns"/>

        <Button fx:id="printReceiptsButton" layoutX="397.0" layoutY="225.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Print Duplicate Receipts" onAction="#handlePrintReceipts"/>

        <Button fx:id="trackQueueButton" layoutX="89.0" layoutY="293.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Track Customer Queue" onAction="#handleTrackQueue"/>

        <Button fx:id="handleComplaintsButton" layoutX="400.0" layoutY="293.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="278.0" text="Handle Complaints" onAction="#handleComplaints"/>

        <Button fx:id="logoutButton" layoutX="216.0" layoutY="384.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="355.0" text="Logout" onAction="#handleLogout"/>

        <Text layoutX="314.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cashier">
            <font>
                <Font size="43.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
